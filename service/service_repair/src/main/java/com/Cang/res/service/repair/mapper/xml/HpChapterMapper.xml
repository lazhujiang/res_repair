<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Cang.res.service.repair.mapper.HpChapterMapper">

    <select id="getResourceById" resultType="com.Cang.res.service.repair.entity.HpChapter">
        SELECT id,resource_id,title,resource_file,gmt_create,gmt_modified FROM hp_chapter WHERE resource_id = #{resourceId}
    </select>
</mapper>
